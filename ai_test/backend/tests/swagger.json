      },
      "delete": {
        "tags": [
          "store"
        ],
        "summary": "删除订单",
        "description": "有效ID必须为正整数，非整数值将引发API错误",
        "operationId": "deleteOrder",
        "produces": [
          "application/json",
          "application/xml"
        ],
        "parameters": [
          {
            "name": "orderId",
            "in": "path",
            "description": "需要删除的订单ID",
            "required": true,
            "type": "integer",
            "minimum": 1,
            "format": "int64"
          }
        ],
        "responses": {
          "400": {
            "description": "无效的ID"
          },
          "404": {
            "description": "未找到订单"
          }
        }
      }
    },
